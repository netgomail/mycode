{
  "name": "mycode",
  "version": "0.5.0",
  "main": "src/app.tsx",
  "dependencies": {
    "ink": "6.8.0",
    "react": "19.2.0"
  },
  "description": "AI-ассистент для разработки (учебный проект)",
  "keywords": [
    "cli",
    "ai",
    "assistant"
  ],
  "license": "MIT",
  "pkg": {
    "assets": [
      "dist/yoga.wasm"
    ],
    "targets": [
      "node22-win-x64",
      "node22-linux-x64",
      "node22-macos-x64"
    ]
  },
  "scripts": {
    "start": "bun src/app.tsx",
    "build:win": "bun run bunbuild.mjs",
    "build:linux": "bun run bunbuild.mjs --linux",
    "build:mac": "bun run bunbuild.mjs --mac",
    "build:all": "bun run bunbuild.mjs --all"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "bun-types": "^1.3.9"
  }
}
